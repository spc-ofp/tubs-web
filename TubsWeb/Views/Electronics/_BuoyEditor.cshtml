<table id="buoyTable" class="table table-bordered table-condensed table-striped">
    <thead>
        <tr>
            <th>Device Type</th>
            <th>Installed</th>
            <th>Usage</th>
            <th>Make</th>
            <th>Model</th>
            <th>How Many</th>
            <th>Comments</th>
            <th>&nbsp;</th>
        </tr>
    </thead>
    <tbody data-bind="foreach: $root.Buoys">
        <tr>
            <td><select class="input-large" data-bind="hasfocus: NeedsFocus, options: $root.BuoyTypes, value: DeviceType"></select></td>
            @*<td><input type="text" class="input-medium" data-bind="hasfocus: NeedsFocus, value: DeviceType, uniqueName: true" /></td>*@
            <td><select class="input-small" data-bind="options: $root.BooleanValues, value: IsInstalled"></select></td>
            <td><select class="input-small" data-bind="options: $root.UsageCodes, value: Usage"></select></td>
            <td><input type="text" class="input-medium" data-bind="value: Make, uniqueName: true" /></td>
            <td><input type="text" class="input-medium" data-bind="value: Model, uniqueName: true" /></td>
            <td><input type="number" class="input-mini" data-bind="value: BuoyCount, uniqueName: true" /></td>
            <td><input type="text" class="input-large" data-bind="value: Comments, uniqueName: true" /></td>
            <td>
                <button type="button" class="btn btn-danger removeItem" tabindex="5000"><i class="icon-trash icon-white"></i></button>
            </td>
        </tr>
    </tbody>
</table>

<button id="addNewBuoy" type="button" class="btn btn-info addItem"><i class="icon-plus-sign icon-white"></i> Add Buoy</button>
<hr />
<button type="button" class="btn btn-info nextTabButton">Next Section <i class="icon-forward"></i></button>