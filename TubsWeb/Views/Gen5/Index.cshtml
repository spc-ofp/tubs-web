@model IEnumerable<TubsWeb.ViewModels.Gen5ViewModel>

@{ ViewBag.Title = "GEN-5 Details"; }

@* For now, create this in the View. *@
@{
    IEnumerable<Tuple<string, string>> crumbs = new List<Tuple<string, string>>()
    {
        Tuple.Create(Url.Action("Index", "Trip"), "Trips"),
        Tuple.Create(Url.Action("Details", "Trip", new { tripId = ViewBag.TripId }), ViewBag.TripNumber ?? "This Trip"),
        Tuple.Create("#", "All GEN-5 Records")
    };
}

@{ Html.RenderPartial("_BreadCrumbs", crumbs); }

@if (Model.Count() == 0)
{
    @: No FADS recorded for this trip
}
else
{
    Html.RenderPartial("_Gen5List", Model);
}